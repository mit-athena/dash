#define XSUBDIRS src

#define LDEF src/Xj_local.defs

DASH_MENUS = ${ATHLIBDIR}/Dash.menus
DASH_FALLBACK = ${ATHLIBDIR}/Dash.fallback
QUICK_FILE = ${ATHLIBDIR}/quickstations

Makefiles::
	-@if [ -f LDEF ]; then \
	echo "  $(RM) LDEF.bak; mv LDEF LDEF.bak"; \
	$(RM) LDEF.bak; mv LDEF LDEF.bak; \
	else exit 0; fi
	@echo "/* This file automatically generated." > LDEF
	@echo " * See ../Imakefile for details." >> LDEF
	@echo " */" >> LDEF
	@echo HOSTTYPE = ${HOSTTYPE} >> LDEF
	@echo MAN_DIR = ${ATHMANDIR} >> LDEF
	@echo DASH_BIN_DIR = ${ATHBINDIR} >> LDEF
	@echo DASH_XBM_DIR = ${ATHXBITDIR} >> LDEF
	@echo DASH_APP_DIR = ${ATHXAPPDIR} >> LDEF
	@echo CONSOLE_BIN_DIR = ${ATHRETCDIR} >> LDEF
	@echo CONSOLE_XBM_DIR = ${ATHRETCDIR}/login/bitmaps >> LDEF
	@echo CONSOLE_APP_DIR = ${ATHRETCDIR}/login >> LDEF
	@echo XJ_XBM_DIR = ${ATHXBITDIR} >> LDEF
	@echo XJ_APP_DIR = ${ATHXAPPDIR} >> LDEF
	@echo XJ_LIB_DIR = ${ATHLIBDIR} >> LDEF
	@echo XJ_INC_DIR = ${ATHINCDIR} >> LDEF
	@echo DASH_MENUS = ${DASH_MENUS} >> LDEF
	@echo DASH_FALLBACK = ${DASH_FALLBACK} >> LDEF
	@echo QUICK_FILE = ${QUICK_FILE} >> LDEF
	@echo LOCAL_INCLUDES = -I${ATHTOOLROOT}${ATHINCDIR} \
		-I${ATHTOOLROOT}${ATHINCDIR}/kerberosIV >> LDEF
	@echo LOCAL_LIBS = -L${ATHTOOLROOT}${ATHLIBDIR} >> LDEF
